{
  "compileOnSave": false,
  "exclude": [
    "./dist/*"
  ],
  "compilerOptions": {
    "baseUrl": "src/",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "@shared": [
        "app/shared"
      ],
      "@shared/*": [
        "app/shared/*"
      ],
      "@testing": [
        "testing"
      ],
      "@testing/*": [
        "testing/*"
      ],
      "@env": [
        "environments"
      ],
      "@env/*": [
        "environments/*"
      ],
      "store": [
        "../dist/store"
      ],
      "store/*": [
        "../dist/store/*"
      ],
      "utils": [
        "../dist/utils"
      ],
      "utils/*": [
        "../dist/utils/*"
      ],
      "auth": [
        "../dist/auth"
      ],
      "auth/*": [
        "../dist/auth/*"
      ]
    }
  }
}

{
  "name": "my-travel-client",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "build": "ng build --prod --build-optimizer",
    "test": "ng test",
    "lint": "npm run lint:ts && npm run lint:style",
    "e2e": "ng e2e",
    "analyze": "ng build --prod --build-optimizer --stats-json",
    "test-coverage": "ng test --code-coverage --watch=false",
    "lint:ts": "tslint -p src/tsconfig.app.json -c tslint.json 'src/**/*.ts'",
    "lint:style": "stylelint \"{src}/**/*.less\" --syntax less",
    "lint-staged": "lint-staged",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "hmr": "ng serve -c=hmr",
    "build:utils": "ng build utils",
    "build:web": "ng build web",
    "build:mobile": "ng build mobile",
    "build:store": "ng build store"
  },
  "private": true,
  "dependencies": {
    "@angular-redux/store": "^9.0.0",
    "@angular/animations": "^6.0.3",
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/router": "^6.0.3",
    "@delon/abc": "^1.3.0",
    "@delon/acl": "^1.3.0",
    "@delon/auth": "^1.3.0",
    "@delon/cache": "^1.3.0",
    "@delon/form": "^1.3.0",
    "@delon/mock": "^1.3.0",
    "@delon/theme": "^1.3.0",
    "@delon/util": "^1.3.0",
    "@ngx-translate/core": "^10.0.2",
    "@types/bson": "^1.0.11",
    "ajv": "^6.4.0",
    "bson": "^2.0.8",
    "core-js": "^2.5.4",
    "editorconfig-tools": "^0.1.1",
    "flux-standard-action": "^2.0.3",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.2",
    "ng-alain": "^1.3.0",
    "ng-zorro-antd": "^1.3.0",
    "normalizr": "^3.2.4",
    "prettier": "^1.12.1",
    "prettier-stylelint": "^0.4.2",
    "qrious": "^4.0.2",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-observable": "^1.0.0",
    "redux-seamless-immutable": "^0.4.0",
    "rxjs": "^6.0.0",
    "screenfull": "^3.3.1",
    "seamless-immutable": "^7.1.3",
    "stylelint": "^9.2.0",
    "stylelint-config-standard": "^18.2.0",
    "tslint-config-prettier": "^1.12.0",
    "tslint-language-service": "^0.9.9",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^6.0.3",
    "@angular-devkit/build-ng-packagr": "~0.6.8",
    "@angular-devkit/build-angular": "~0.6.8",
    "ng-packagr": "^3.0.0-rc.2",
    "tsickle": ">=0.25.5",
    "tslib": "^1.7.1",
    "typescript": "~2.7.2",
    "@angular/cli": "~6.0.8",
    "@angular/language-service": "^6.0.3",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "less": "~2.7.0",
    "less-loader": "~4.0.0",
    "@angularclass/hmr": "^2.1.3",
    "@types/redux-logger": "^3.0.6",
    "@types/seamless-immutable": "^7.1.2"
  },
  "lint-staged": {
    "*.{cmd,html,json,md,sh,txt,xml,yml}": [
      "editorconfig-tools fix",
      "git add"
    ],
    "*.ts": [
      "npm run lint:ts",
      "prettier --write",
      "git add"
    ],
    "*.less": [
      "npm run lint:style",
      "prettier --write",
      "git add"
    ]
  }
}
